    "numerator=table['XY'].loc['Sigma']-table['X'].loc['Sigma']*table['Y'].loc['Sigma']/20\n",
    "denominator=table['X^2'].loc['Sigma']-table['X'].loc['Sigma']**2/20\n",
    "b=numerator/denominator\n",
    "print(\"b=\",b)"
   ]
