    "table=pd.DataFrame(df.iloc[6,1:])\n",
    "table.columns=['Y']\n",
    "table['X']=range(1,21)\n",
    "table['XY']=table['Y']*table['X']\n",
    "table['X^2']=table['X']**2\n",
    "sum=pd.DataFrame(table.sum())\n",
    "sum.columns=['Sigma']\n",
    "sum\n",
    "mean=pd.DataFrame(table.mean())\n",
    "mean.columns=['Mean']\n",
    "mean\n",
    "table=pd.concat([table,sum.T,mean.T])\n",
    "table\n"
   ]
